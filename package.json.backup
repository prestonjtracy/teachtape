    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/nodemailer": "^7.0.0",
    "@types/react": "18.3.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
