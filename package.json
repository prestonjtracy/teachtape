{
  "name": "teachtape-starter",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run check-env",
    "build": "next build",
    "start": "next start",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check-env": "node --env-file=.env.local scripts/check-env.ts || node scripts/check-env.ts",
    "normalize-env": "node scripts/normalize-env.mjs",
    "seed:coaches": "tsx scripts/seed-coaches.ts",
    "seed": "tsx scripts/seed.ts",
    "test:booking": "tsx scripts/test_booking_e2e.ts",
    "dev:all": "concurrently \"next dev\" \"stripe listen --forward-to localhost:3000/api/stripe/webhook\"",
    "predeploy": "npm run typecheck && npm run build",
    "deploy:check": "tsx scripts/pre-deployment-check.ts"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^4.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "2.45.4",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "isomorphic-dompurify": "^2.28.0",
    "next": "14.2.5",
    "nodemailer": "^6.9.13",
    "react": "18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "18.3.1",
    "resend": "^3.4.0",
    "stripe": "16.9.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "20.11.30",
    "@types/nodemailer": "^7.0.0",
    "@types/react": "18.3.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "4.7.0",
    "typescript": "5.4.5"
  }
}
